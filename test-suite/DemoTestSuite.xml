<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Restful Booker Test Suite">
    <listeners>
        <listener class-name="assured.com.listeners.TestListener"/>
    </listeners>

    <test name="Restful create tests">
        <classes>
            <class name="assured.com.testcases.UserTestCase">
                <methods>
                    <include name="createUser"/>
